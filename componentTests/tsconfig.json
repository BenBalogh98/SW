{
    "compilerOptions": {
        "target": "es6",
        "jsx": "react-jsx",
        "module": "commonjs",
        "lib": ["es6", "dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": false
    },
    "exclude": ["cypress/support/commands.js", "cypress/support/component.js", "cypress/support/e2e.js"],
    "include": ["cypress"]
    
}